package com.palm360.airport.util.stereotype;

import java.io.Serializable;

/**
 * 用户记录外键对应的表，字段名，value
 * @author zhangtong
 *
 */
@Deprecated
public class ForeignKeyObject implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 3844775619323483578L;
	private String tableName;
	private String fkId;
	private String fkFieldName;
	/**
	 * 对应的外键子项的字段名
	 */
	private String fieldName;
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	public String getTableName() {
		return tableName;
	}
	public void setFkId(String fkId) {
		this.fkId = fkId;
	}
	public String getFkId() {
		return fkId;
	}
	public void setFieldName(String fieldName) {
		this.fieldName = fieldName;
	}
	public String getFieldName() {
		return fieldName;
	}
	public void setFkFieldName(String fkFieldName) {
		this.fkFieldName = fkFieldName;
	}
	public String getFkFieldName() {
		return fkFieldName;
	}
}
